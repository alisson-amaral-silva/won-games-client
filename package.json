{
  "name": "react-avancado-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=3",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "test:ci": "jest --runInBand",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test dev 3000 cy:open",
    "test:e2e:ci": "start-server-and-test start 3000 cy:run",
    "schema:download": "apollo client:download-schema --endpoint=http://localhost:1337/graphql schema-temp.gql",
    "schema:convert": "node convertToNonNull.js",
    "codegen": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema.gql --tagName=gql --outputFlat src/graphql/generated",
    "types:generate": "yarn schema:download && yarn schema:convert && yarn codegen",
    "generate": "yarn plop --plopfile generators/plopfile.js",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook -s ./public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test --findRelatedTests --bail"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "@stripe/react-stripe-js": "^1.4.1",
    "@stripe/stripe-js": "^1.15.1",
    "@styled-icons/boxicons-regular": "^10.34.0",
    "@styled-icons/boxicons-solid": "^10.34.0",
    "@styled-icons/fa-brands": "^10.34.0",
    "@styled-icons/material-outlined": "^10.34.0",
    "@styled-icons/remix-fill": "^10.18.0",
    "@types/lodash.xor": "^4.5.6",
    "dotenv": "^10.0.0",
    "graphql": "^15.5.1",
    "joi": "^17.4.0",
    "lodash.xor": "^4.5.0",
    "next": "11.0.1",
    "next-auth": "^3.27.0",
    "next-pwa": "^5.2.21",
    "next-seo": "^4.26.0",
    "nextjs-progressbar": "^0.0.11",
    "polished": "^4.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-slick": "^0.28.1",
    "storybook-addon-next-router": "^2.0.4",
    "styled-components": "5.3.0",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@next/eslint-plugin-next": "^11.1.2",
    "@storybook/addon-essentials": "^6.1.17",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.1.17",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@types/jest": "^27.0.1",
    "@types/next-auth": "3.7.0",
    "@types/node": "^16.7.1",
    "@types/react": "^17.0.19",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.15.0",
    "apollo": "^2.33.4",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.8",
    "jest": "^27.0.6",
    "jest-styled-components": "^7.0.3",
    "lint-staged": "^10.5.3",
    "msw": "^0.35.0",
    "node-fetch": "^3.0.0",
    "plop": "^2.7.4",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  }
}
